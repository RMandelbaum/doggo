<% if !logged_in? %>
  <li><%= link_to "Sign Up", new_user_path %></li>
  <li><%= link_to "Sign In", login_path %></li>
  <li><%= link_to('Log in with Facebook!', '/auth/facebook') %></li>
<% else %>
  <li><%= link_to "Welcome #{current_user.username}!", user_path(current_user) %></li>
  <li><%= link_to "View Dogs", dogs_path %></li>
  <li><%= link_to "Reserved Walks", user_walks_path(current_user) %></li>
  <li><%= link_to "Log Out", logout_path %></li>
<% end %>
