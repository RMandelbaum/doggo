<% if !logged_in? %>
  <li><%= link_to "Sign Up", new_user_path %></li>
  <li><%= link_to "Log In", login_path %></li>
  <li><%= link_to('Log in with Facebook!', '/auth/facebook') %></li>
<% elsif current_user.admin %>
  <li><%= link_to "View Dogs", dogs_path %></li>
  <li><%= link_to "View Walkers", users_path %></li>
  <li><%= link_to "Urgent Walks", walks_urgent_walks_path %></li>
  <li><%= link_to "Log Out", logout_path %></li>
  <% else %>
  <li><%= link_to "Profile", user_path(current_user) %></li>
  <li><%= link_to "View Dogs", dogs_path %></li>
  <li><%= link_to "Reserved Walks", user_walks_path(current_user) %></li>
  <li><%= link_to "Urgent Walks",  walks_urgent_walks_path %></li>
  <li><%= link_to "Log Out", logout_path %></li>
<% end %>
