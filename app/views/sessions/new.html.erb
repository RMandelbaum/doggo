<div class="container">

  <%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>

      <%= f.label :username %><br>
      <%= f.text_field :username %><br>

      <%= f.label :password %><br>
      <%= f.password_field :password %><br>


      <%= f.submit "Sign In" %>
      <% end %>
  </div>
