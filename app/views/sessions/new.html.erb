<div class="container">
  <h1>Log In</h1>

<%= form_for(:session, url: login_path) do |f| %>
      <%= render '/sessions/error' %>
      <br>

      <%= f.label :username %><br>
      <%= f.text_field :username %><br>

      <%= f.label :password %><br>
      <%= f.password_field :password %><br>


      <%= f.submit "Sign In" %>
      <% end %>
  </div>
