<div class= "container">
  <h2>Create a new walk for <%= @dog.name %></h2>
    <%= form_for @walk, url: dog_path(@dog) do |f| %>
      <%= f.label :day %>
      <%= f.text_field :day %> <br>
      <%= f.label :time %>
      <%= f.text_field :time %> <br>
      <%= f.submit %>
    <% end %>
    </div>
