<%= form_for dog do |f| %>
  <%= f.fields_for :walks, walk do |w| %>
    <%= w.hidden_field :reserved, value: !walk.reserved %>
    <%= w.hidden_field :user_id, value: current_user.id if walk.reserved %>
    <% end %>
    <% if !walk.reserved %>
<%= f.submit "Reserve"%>
<% else %>
<%= f.submit "Unreserve" %>
<% end %>
<% end %>
