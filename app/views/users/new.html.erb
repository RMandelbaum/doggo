  <div class="container">
    <h1>Sign Up</h1>
      <div>
        <%= form_for @user do |f| %>
            <%= render partial: '/sessions/error', locals: {current_user: @user} %>
            <br>
              <%= f.label :username %><br>
              <%= f.text_field :username%><br>

              <%= f.label :email%><br>
              <%= f.email_field :email%><br>

              <%= f.label :password %><br>
              <%= f.password_field :password %><br>

         <%= f.submit  %>
        <% end %>
    </div>
    </div>
